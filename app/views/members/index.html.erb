  <div class="ui vertical icon menu blue visible-xs-inline-block visible-sm-inline-block" style="position:fixed; right:0px; top:200px;">
    <a class="item filter active twitter sort poop" data-sort="random" data-content="Shuffle" data-position="left center">
      <i class="refresh icon"></i>
    </a>  
    <a class="item filter poop" data-filter="all" data-content="All" data-position="left center">
      <i class="table icon"></i>
    </a>
    <a class="item filter poop" data-filter=".chair" data-content="Chairman" data-position="left center">
      <i class="block layout icon"></i>
    </a>
    <a class="item filter poop" data-filter=".gov" data-content="Governors" data-position="left center">
      <i class="grid layout icon"></i>
    </a>
